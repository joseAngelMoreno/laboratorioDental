<odoo>
    <data>

        <record model="ir.ui.view" id="laboratorio_dental.precios_tree">
            <field name="name">Precios</field>
            <field name="model">laboratorio_dental.precios_model</field>
            <field name="arch" type="xml">
            
              <tree>
                <field name="name"/>
                <field name="descripcion"/>
                <field name="precio"/>

              </tree>
            </field>
          </record>


          <record model="ir.ui.view" id="laboratorio_dental.precios_form">
            <field name="name">Formulario de Precios</field>
            <field name="model">laboratorio_dental.precios_model</field>
            <field name="arch" type="xml">
              <form >
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="descripcion"/>
                        <field name="precio"/>
                    </group>
                    
                </sheet>
              </form>
            </field>
          </record>


         


          <record model="ir.actions.act_window" id="laboratorio_dental.action_window_precios">
            <field name="name">Precios</field>
            <field name="res_model">laboratorio_dental.precios_model</field>
            <field name="view_mode">tree,form</field>
          </record>

        
        </data>
        </odoo>